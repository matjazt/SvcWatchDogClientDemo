{
  "log": {
    "consoleLevel": 99,
    "fileLevel": 0,
    "filePath": "log\\SvcWatchDogClientDemoService.log",
    "maxFileSize": 10000000,
    "maxOldFiles": 5,
    "maxWriteDelay": 500,
    "logThreadId": false
  },
  "svcWatchDog": {
    "workDir": "..",
    "args": [
      "python.exe",
      "src\\svc_watch_dog_client_demo.py"
    ],
    "usePath": true,
    "autoStart": true,
    "restartDelay": 5000,
    "shutdownTime": 3000,
    "watchdogTimeout": 60000
  }
}